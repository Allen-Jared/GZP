<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LoginScreenController">
    <AnchorPane fx:id="header">
        <children>
            <Label fx:id="location"/>
            <Label fx:id="title"/>
        </children>
    </AnchorPane>
    <AnchorPane fx:id="body">
        <children>
            <Label fx:id="login"/>
            <Label fx:id="username"/>
            <TextField fx:id="usernameText"/>
            <Label fx:id="password"/>
            <TextField fx:id="passwordText"/>
            <Button fx:id="loginButton" onAction="#loginButtonClick"/>
        </children>
    </AnchorPane>
</AnchorPane>
